/**
 * User class pretty much just contains getters and setters that aid the login
 */

public class User {
    String username;
    String password;

    public User(String u, String p) {
        username = u;
        password = p;
    }

    // getters
    public String getUser() {
        return username;
    }

    public String getPass() {
        return password;
    }

    // setters
    public void setPass(String p) {
        password = p;
    }

}
